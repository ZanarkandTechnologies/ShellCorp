{
  "name": "ShellCorp",
  "version": "0.1.0",
  "private": true,
  "description": "ShellCorp - single-brain, multi-hand AI operating system",
  "type": "module",
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "build": "tsc --noEmit",
    "typecheck": "tsc --noEmit",
    "dev": "tsx src/cli/index.ts",
    "gateway": "tsx src/cli/index.ts gateway",
    "gateway:with-ui": "concurrently -k \"GATEWAY_UI_URL=http://127.0.0.1:5173 pnpm run gateway\" \"pnpm run ui\"",
    "agent": "tsx src/cli/index.ts agent --message \"health check\"",
    "status": "tsx src/cli/index.ts status",
    "ui": "vite --config ui/vite.config.ts",
    "ui:build": "vite build --config ui/vite.config.ts",
    "test": "vitest",
    "test:once": "vitest run",
    "test:debug": "vitest --inspect-brk --no-file-parallelism",
    "test:coverage": "vitest run --coverage --coverage.reporter=text"
  },
  "dependencies": {
    "@mariozechner/pi-agent-core": "latest",
    "@mariozechner/pi-ai": "latest",
    "@mariozechner/pi-coding-agent": "latest",
    "@notionhq/client": "^5.9.0",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-avatar": "^1.1.11",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-hover-card": "^1.1.15",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-progress": "^1.1.8",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@radix-ui/react-use-controllable-state": "^1.2.2",
    "@react-three/drei": "^10.7.7",
    "@react-three/fiber": "^9.5.0",
    "@slack/bolt": "latest",
    "@whiskeysockets/baileys": "^6.17.16",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "commander": "latest",
    "convex": "^1.32.0",
    "croner": "latest",
    "discord.js": "latest",
    "grammy": "latest",
    "lucide-react": "^0.575.0",
    "next-themes": "^0.4.6",
    "qrcode-terminal": "^0.12.0",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "react-router-dom": "^7.13.1",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.5.0",
    "tailwindcss": "^4.2.1",
    "three": "^0.183.1",
    "three-pathfinding": "^1.3.0",
    "tw-animate-css": "^1.4.0",
    "zod": "latest",
    "zustand": "^5.0.11"
  },
  "devDependencies": {
    "@edge-runtime/vm": "^5.0.0",
    "@types/node": "latest",
    "@types/qrcode-terminal": "^0.12.2",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.4",
    "concurrently": "^9.2.1",
    "convex-test": "^0.0.41",
    "tsx": "latest",
    "typescript": "latest",
    "vite": "^7.3.1",
    "vitest": "^3.2.4"
  }
}
